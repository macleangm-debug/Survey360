      "body_base": "text-base leading-relaxed",
      "caption": "text-xs tracking-wider uppercase font-medium text-muted-foreground"
    },
    "imports": [
      "https://fonts.googleapis.com/css2?family=Barlow+Condensed:wght@400;500;600;700&family=Manrope:wght@300;400;500;600;700&family=JetBrains+Mono:wght@400;500&display=swap"
