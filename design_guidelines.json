      "glass_bg_dark": "rgba(11, 13, 18, 0.6)"
    }
  },
  "visual_enhancers": {
    "borders": {
      "default": "border border-border",
      "glass": "border border-white/10"
    },
    "shadows": {
      "subtle": "shadow-sm",
      "glow": "shadow-[0_0_20px_-5px_rgba(79,70,229,0.3)]",
      "card": "shadow-xl ring-1 ring-black/5"
    },
    "radius": {
      "default": "rounded-sm",
      "card": "rounded-md",
      "button": "rounded-sm"
    },
    "gradients": {
      "hero_overlay": "bg-gradient-to-b from-transparent via-background/80 to-background",
      "active_beam": "bg-gradient-to-r from-transparent via-primary/50 to-transparent"
    }
  },
  "layout_strategy": {
    "dashboard_grid": "grid grid-cols-1 md:grid-cols-12 gap-4 auto-rows-min",
    "bento_grid": {
      "wrapper": "grid grid-cols-1 md:grid-cols-4 gap-4",
      "hero_cell": "md:col-span-2 md:row-span-2",
      "tall_cell": "md:col-span-1 md:row-span-2",
      "wide_cell": "md:col-span-2 md:row-span-1"
    },
    "spacing": {
      "section_padding": "py-12 md:py-24",
      "container_padding": "px-4 md:px-8",
      "card_padding": "p-6"
    }
  },
  "component_rules": {
    "cards": {
      "style": "bg-card/50 backdrop-blur-md border border-border/50",
      "hover": "hover:border-primary/50 hover:bg-card/80 transition-all duration-300"
    },
    "buttons": {
      "primary": "bg-primary text-primary-foreground hover:bg-primary/90 shadow-lg shadow-primary/20",
      "outline": "border border-input bg-transparent hover:bg-accent hover:text-accent-foreground",
      "ghost": "hover:bg-accent hover:text-accent-foreground"
    },
    "inputs": {
      "style": "bg-background/50 border-input focus:ring-2 focus:ring-primary/20 focus:border-primary transition-all"
    }
  },
