  "libraries": {
    "charts": "recharts",
    "maps": "react-map-gl (if 3D needed) or leaflet/react-leaflet (for lightweight)",
    "tables": "@tanstack/react-table",
    "forms": "react-hook-form + zod",
    "animation": "framer-motion"
  },
  "universal_guidelines": [
    "TACTICAL PRECISION: Use monospace fonts for all data/numbers.",
    "DARK MODE FIRST: Design complex views in dark mode first, then invert for light mode.",
    "NO GRADIENT TEXT: Keep text solid for readability against complex data backgrounds.",
    "BORDERS OVER SHADOWS: In dark mode, use 1px subtle borders (white/10) to define hierarchy, not shadows.",
    "DENSE BUT BREATHABLE: Use 'compact' mode for tables (py-2) but 'comfortable' mode for page layout (gap-6).",
    "INTERACTIVE FEEDBACK: Every click must have visual feedback (ripple or scale-down).",
    "ACCESSIBILITY: Ensure 4.5:1 contrast for all text. Use 'sonner' for toast notifications."
  ],
  "instructions_to_main_agent": [
    "Install 'lucide-react', 'recharts', 'framer-motion', 'clsx', 'tailwind-merge' if not present.",
    "Use the provided image URLs for specific sections.",
    "Implement the 'Barlow Condensed' font for all headings.",
    "Follow the 'Bento Grid' strategy for the main dashboard overview.",
    "Ensure the 'Form Builder' uses a clean, spreadsheet-like layout (minimal borders, high focus)."
  ]
